        {
            "name": "Merge requests",
            "type": "merge_requests",
            "scope": "all",
            "state": "opened",
            "noItemText": "No merge request in project."
        }
    ],
    "rest-client.decodeEscapedUnicodeCharacters": true,
    "rest-client.previewResponseInUntitledDocument": true,
    "rest-client.requestNameAsResponseTabTitle": true,
    "vscode-postgres.prettyPrintJSONfields": true,
    "search.useParentIgnoreFiles": true,
