    "python.createEnvironment.trigger": "off",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
    },
    "markdown-preview-enhanced.enablePreviewZenMode": true,
    "markdown-preview-enhanced.previewTheme": "one-dark.css",
    "markdown-preview-enhanced.codeBlockTheme": "github-dark.css",
    "editor.cursorSurroundingLines": 5,
    "platformio-ide.autoOpenPlatformIOIniFile": false,
    "auto-build.defaultEnv.name": "STM32F103RE_creality",
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
    },
    "terminal.integrated.fontLigatures.enabled": true,
    "groupImports.onSave": true,
    "workbench.editor.editorActionsLocation": "hidden",
    "problems.autoReveal": false,
    "workbench.editor.empty.hint": "hidden",
    "C_Cpp.vcFormat.newLine.beforeElse": false,
    "C_Cpp.vcFormat.newLine.beforeCatch": false,
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
    "workbench.activityBar.location": "top",
    "git.blame.editorDecoration.enabled": false,
    "workbench.sideBar.location": "right",
    "chat.commandCenter.enabled": false,
    "vscode_custom_css.imports": [
        "file:///home/lohopupa/.dotfiles/vscode/style.css",
        "file:///home/lohopupa/.dotfiles/vscode/load.js"
    ],
    "redhat.telemetry.enabled": false,
    "git.blame.statusBarItem.enabled": true,
    "workbench.colorTheme": "Bearded Theme Monokai Reversed",
    "editor.multiCursorModifier": "ctrlCmd",
    "telemetry.telemetryLevel": "off",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/node_modules/**": true,
        "**/dist/**": true,
        "**/.next/**": true,
        "**/build/**": true
    },
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": false,
    },
    "editor.quickSuggestionsDelay": 10,
    "workbench.reduceMotion": "on",
    "gitlab.showPipelineUpdateNotifications": true,
    "extensions.ignoreRecommendations": true,
    "rest-client.enableTelemetry": false,
    "git.openRepositoryInParentFolders": "never",
